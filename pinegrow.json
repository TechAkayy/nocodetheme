{
  "open-pages": ["index.html"],
  "breakpoints": ["640px", "768px", "1024px", "1280px", "1536px"],
  "frameworks": [
    "nocodetheme-wp",
    "pg.tree.painter",
    "pg.tw.lib",
    "pg.asset.manager",
    "pg.project.items",
    "pg.code-validator",
    "pg.image.overlay",
    "pg.css.grid",
    "wordpress.pinegrow",
    "pg.svg.lib",
    "pg.ia.lib",
    "pg.insight.events",
    "pg.ia",
    "tw.flowbite",
    "tw",
    "pg.html",
    "pg.components"
  ],
  "tw-use-external-build": "1",
  "recent-classes": [],
  "active-design-provider": "tw",
  "template_framework_id": "pg.tw.lib",
  "design-settings": {
    "mode": "external",
    "external_source_url": "src/assets/css/tailwind.css",
    "external_help": null,
    "external_config_path": "tailwind.config.js",
    "external_theme_path": "themes/pg-tailwindcss/tokens.cjs",
    "custom_config": null,
    "add_setting": null,
    "custom_source_url": null,
    "disable_preflight": null,
    "external_config_url": "tailwind.config.js",
    "ignore_skill_preflight_setting": "1"
  },
  "class-styles-components": "1",
  "wp-theme-info": {
    "dir": "${nocodetheme}",
    "name": "nocodetheme",
    "slug": "nocodetheme",
    "url": "${nocodetheme_url}",
    "project_version": "0.0.3",
    "auto_increment_version": "0",
    "page": "index.html",
    "themejson": "true",
    "wpseo": "true",
    "wpjquery": "false",
    "generator": "true",
    "images": {},
    "blocks_type": "native-auto",
    "pg_version": "8.0",
    "blocks_editor_styles": [],
    "project_type": "theme",
    "blocks_inline_svg": "false",
    "blocks_load_if_rendered": "false",
    "wc_enabled": "false"
  },
  "wp-insight": {
    "menus": ["primary", "social"],
    "taxonomies": ["category", "departments", "post_tag"],
    "customizer_sections": ["social_links"],
    "customizer_controls": [
      "facebook_link",
      "github_link",
      "instagram_link",
      "logo_light",
      "twitter_link",
      "youtube_link"
    ],
    "post_types": ["attachment", "contact_form", "employees", "page", "post"],
    "template_parts": [
      "template-parts/employees-sidebar",
      "template-parts/pagination",
      "template-parts/sidebar",
      "template-parts/social-icons"
    ],
    "sidebars": ["company-information"],
    "master_pages": ["archive-employees"],
    "image_sizes": [
      "full",
      "large",
      "medium",
      "medium_large",
      "post-thumbnail",
      "thumbnail"
    ],
    "custom_funcs": ["the_archive_title()"],
    "block_categories": ["custom-blocks"]
  },
  "wp-files": {
    "jsconfig.json": "false",
    "package-lock.json": "false",
    "package.json": "false",
    "postcss.config.cjs": "false",
    "vite-plugin-pg.js": "false",
    "vite.config.js": "false",
    "src": "false",
    "tailwind_theme/tailwind.css": "true",
    "tailwind_theme": "true",
    "tailwind_theme/tailwind_for_wp_editor.css": "true"
  },
  "automatic-tailwindcss-builder": false
}
